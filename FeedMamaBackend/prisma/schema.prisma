// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlserver"
  url      = env("DATABASE_URL")
}

model Users {
  UserId String @db.UniqueIdentifier @id
  Email String
  LastName String
  FirstName String
  Phone String
  Password String
  Address String
  City String
  Role Int
  ZIP String
}
